{
  "expo": {
    "name": "FoodHauza",
    "slug": "FoodHauza",
     "scheme": "FoodHauza",
    "owner": "gimase",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/donation.png",
    "splash": {
      "image": "./assets/donation.png",
      "resizeMode": "contain",
      "backgroundColor": "#5AAE7F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    }
  }
}
